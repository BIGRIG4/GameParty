<!-- events-index -->
 <div class="row mt-4">
     <div class="col-lg-9 offset-lg-2">
       <h5>ğŸ‘‡ğŸ»ğŸ¤–Create a gaming event below</h5>
       <a href="/events/new">Start A Party</a>
         <h1>Current Gaming Events</h1>
         {{#each events}}
          <div class="card mt-5">
            <img class="card-img-top" src="{{this.imgUrl}}" alt="Card image cap">
              <div class="card-body">
                <h5 class="card-title">{{this.title}}</h5>
                <p class="card-text">{{this.description}}</p>
                <div class="text-right">
                  <a href="/events/{{this.id}}/rsvps/new" class="btn btn-info">Join The Party Here</a>
                  <a href="/events/{{this.id}}" class="btn btn-primary">Current Gamers</a>
                </div>
              </div>
            </div>
          {{/each}}
     </div>
 </div>
